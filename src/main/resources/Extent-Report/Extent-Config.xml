<?xml version="1.0" encoding="UTF-8"?>
<extentreports>
    <configuration>
        <!-- report theme -->
        <!-- standard, dark -->
        <theme>standard</theme>

        <!-- document encoding -->
        <!-- defaults to UTF-8 -->
        <encoding>UTF-8</encoding>

        <!-- protocol for script and stylesheets -->
        <!-- defaults to https -->
        <protocol>https</protocol>

        <!-- title of the document -->
        <documentTitle>Product Model Automation Testing Report</documentTitle>

        <!-- report name - displayed at top-nav -->
        <reportName>Product Model Validation V1</reportName>

        <!-- location of charts in the CoolOffInstructions view -->
        <!-- top, bottom -->
        <testViewChartLocation>bottom</testViewChartLocation>

        <!-- settings to enable/disable views -->
        <!-- professional version only -->
        <enableCategoryView>true</enableCategoryView>
        <enableAuthorView>false</enableAuthorView>
        <enableExceptionView>true</enableExceptionView>
        <enableTestRunnerLogsView>true</enableTestRunnerLogsView>

        <!-- custom javascript -->
        <scripts>
            <![CDATA[
                $(document).ready(function() {

                });
            ]]>
        </scripts>

        <!-- custom styles -->
        <styles>
            <![CDATA[

            ]]>
        </styles>
    </configuration>
</extentreports>